{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/_framework/*",
            "/_content/*",
            "/css/*",
            "/js/*",
            "/api/*",
            "*.{ico,png,jpg,gif,svg,css,js,woff,woff2,ttf,eot,dll,pdb,br,dat,blat}"
        ]
    },
    "routes": [
        {
            "route": "/_framework/*",
            "headers": {
                "Cache-Control": "public, max-age=604800, immutable"
            }
        },
        {
            "route": "/_content/*",
            "headers": {
                "Cache-Control": "public, max-age=604800, immutable"
            }
        },
        {
            "route": "/css/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/js/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-eval' 'unsafe-inline' https://open.faceit.com https://cdn.jsdelivr.net; img-src 'self' data: https: https://assets.faceit-cdn.net; font-src 'self' data: https: https://fonts.gstatic.com https://use.fontawesome.com; connect-src 'self' https://open.faceit.com https://fonts.googleapis.com wss://*; style-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net https://fonts.googleapis.com https://use.fontawesome.com;",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "strict-origin-when-cross-origin"
    }
}