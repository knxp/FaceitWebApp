{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET"
            ],
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/_framework/*",
            "headers": {
                "Cache-Control": "must-revalidate, max-age=86400"
            }
        },
        {
            "route": "/css/*",
            "headers": {
                "Cache-Control": "must-revalidate, max-age=86400"
            }
        },
        {
            "route": "/js/*",
            "headers": {
                "Cache-Control": "must-revalidate, max-age=86400"
            }
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-eval' 'unsafe-inline' https://open.faceit.com https://cdn.jsdelivr.net; img-src 'self' data: https:; font-src 'self' data: https: https://fonts.gstatic.com https://use.fontawesome.com; connect-src 'self' https://open.faceit.com https://fonts.googleapis.com; style-src 'self' 'unsafe-inline' https://cdn.jsdelivr.net https://fonts.googleapis.com https://use.fontawesome.com;",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
    }
}