{
    "navigationFallback": {
        "rewrite": "/_Host",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/js/*",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/_framework/*",
            "headers": {
                "cache-control": "must-revalidate, max-age=86400"
            }
        },
        {
            "route": "/_blazor/*",
            "headers": {
                "cache-control": "must-revalidate, max-age=86400"
            }
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/_Host",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-eval' 'unsafe-inline' https://api.faceit.com; img-src 'self' data: https:; font-src 'self' data: https:;"
    },
    "mimeTypes": {
        ".json": "text/json",
        ".woff": "application/font-woff",
        ".woff2": "application/font-woff2"
    }
}